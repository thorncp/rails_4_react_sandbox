<div class="row">
  <div class="page-header">
    <h1>Edit User</h1>
  </div>
</div>

<div class="row">
  <div class="col-xs-12">
    <p id="notice"><%= notice %></p>
  </div>
</div>

<div class="row">
  <div class="col-xs-12">
    <ul class="nav nav-pills">
      <li>
        <%= link_to 'Edit', edit_user_path(@user), role: 'presentation' %>
      </li>
      <li>
        <%= link_to 'Back', users_path, role: 'presentation' %>
      </li>
    </ul>
  </div>
</div>

<div class="row">
  <div class="panel panel-default">
    <div class="panel-heading">User Information</div>
    <div class="panel-body">
      <p>...</p>
    </div>
    <ul class="list-group">
      <li class="list-group-item"><strong>First name: </strong><%= @user.first_name %></li>
      <li class="list-group-item"><strong>Last name: </strong><%= @user.last_name %></li>
      <li class="list-group-item"><strong>Date of birth: </strong><%= @user.date_of_birth %>
      <li class="list-group-item"><strong>Email: </strong><%= @user.email %></li>
    </ul>
  </div>
</div>





